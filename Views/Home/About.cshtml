@model  IEnumerable<BlackCoderUniversityApp.SchoolViewModels.EnrollmentDateGroup>
@{
    ViewData["Title"] = "Statistics";
}

<h2>Student Body Statistics</h2>
<table>
    <tr>
        <th>
            Enrollment Date
        </th>
        <th>
            Students
        </th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.EnrollmentDate)
    </td>
    <td>
        @item.StudentCount
    </td>
</tr>
    }
</table>

<div class="text-center">
    <h1 class="display-4">Hey Coder!! Happy Coding..</h1>
    <p>Check Out -->> <a href="https://www.youtube.com/TrickSoLogic/">Subscribe for more</a>.</p>
</div>
